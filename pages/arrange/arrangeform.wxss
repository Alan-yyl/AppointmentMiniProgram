/* 加载中 */
/* 21.05 24.86 */
.weui-loading {
  /* margin: 0 5px; */
  width: 123rpx;
  height: 50rpx;
  display: flex;
  vertical-align: middle;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.addbutton {
  /* width: 25rpx;
  height: 25rpx; */
  margin-top: 9rpx;
  display: flex;
  margin-left: 10rpx;
}

.minusbutton {
  /* width: 25rpx;
  height: 25rpx; */
  margin-top: -2rpx;
  /* margin-left: 100rpx; */
  margin-right: -20rpx;
  display: flex;
}

