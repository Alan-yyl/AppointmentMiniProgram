/* pages/arrange/arrangeform.wxss */

.form {
  width: 100%;
  border: 3rpx solid #ececec;
  border-radius: 10rpx;
  /* margin: 20rpx; */
  height: 40%;
  background-color: white;
  display: flex;
  flex-wrap: wrap;
}

/* 输入框和loading框的外框 */

.item {
  display: flex;
  /* border: 3rpx solid #; */
  width: 280rpx;
  height: 50rpx;
  padding: 0;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  margin-left: 53rpx;
}

.inputitem {
  /* border-radius:10rpx; */
  color: #bababa;
  height: 50rpx;
  text-align: center;
  border: 3rpx solid #f4ea2a;
  /* margin-left: 0rpx; */
  display: flex;
}

